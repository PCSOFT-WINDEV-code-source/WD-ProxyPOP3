        code : |1+
         nCompte est un entier
         sPOP est une chaine
         sPOPUser est une chaine
         InfoCompte est un xmldocument
         
         
         // Parcourt la table
         POUR nCompte = 1 _A_ TableOccurrence(TABLE_CompteOE)
         	
         	// Si le compte est coché comme devant utilisée le proxy
         	SI COL_bUtiliseProxy[nCompte] ALORS
         		
         		// Si le compte n'utilise pas encore le proxy
         		SI PAS COL_bUtiliseActuel[nCompte] ALORS
         			
         			// On doit créer l'association
         			selon COL_TypeMessagerie
         				CAS "OE"
         					sPOP = RegistreLit(COL_sLocCompte[nCompte], <§$0001§>)
         					sPOPUser = RegistreLit(COL_sLocCompte[nCompte], <§$0002§>)
         					
         					// Pour cela, UserName devient Serveur:UserName
         					RegistreEcrit(COL_sLocCompte[nCompte], <§$0002§>, sPOP +":"+ sPOPUser)
         					RegistreEcrit(COL_sLocCompte[nCompte], <§$0001§>, sGetAdresseEcoute())
         					
         				CAS "WLM"
         					InfoCompte = XMLOuvre(COL_sLocCompte[nCompte], depuisFichier)
         					SI ErreurDétectée = Faux ALORS
         						// Modifie les informations du serveur POP et de l'utilisateur associé
         						sPOP = InfoCompte.MessageAccount.POP3_Server
         						sPOPUser = InfoCompte.MessageAccount.POP3_User_Name
         						
         						InfoCompte.MessageAccount.POP3_User_Name = sPOPUser +"@"+ sPOP
         						InfoCompte.MessageAccount.POP3_Server = sGetAdresseEcoute()
         						XMLSauve(InfoCompte)
         					FIN
         					
         			FIn
         			
         		FIN
         		
         	SINON
         		
         		// Si le compte utilise le compte proxy
         		SI COL_bUtiliseActuel[nCompte] ALORS
         			
         			SELON COL_TypeMessagerie
         				CAS "OE"
         					// On doit retirer l'association
         					sPOP = RegistreLit(COL_sLocCompte[nCompte], <§$0001§>)
         					sPOPUser = RegistreLit(COL_sLocCompte[nCompte], <§$0002§>)
         					
         					// On rétablit les anciens paramètres
         					// On découpe l'identifiant composé
         					nPosSepar est un entier = position(sPOPUser, ":")
         					sServer, sUser sont des chaines
         					SI nPosSepar>0 ALORS
         						sServer = Gauche(sPOPUser, nPosSepar-1)
         						sUser = Milieu(sPOPUser, nPosSepar+1)
         					SINON
         						nPosSepar = Position(sPOPUser, "@")
         						sUser = Gauche(sPOPUser, nPosSepar-1)
         						sServer = Milieu(sPOPUser, nPosSepar+1)
         					FIN
         					
         					// Et on replace les composantes dans la base de registre
         					RegistreEcrit(TABLE_CompteOE.COL_sLocCompte[nCompte], <§$0002§>, sUser)
         					RegistreEcrit(TABLE_CompteOE.COL_sLocCompte[nCompte], <§$0001§>, sServer)
         					
         				CAS "WLM"
         					InfoCompte = XMLOuvre(COL_sLocCompte[nCompte], depuisFichier)
         					SI ErreurDétectée = Faux ALORS
         						// Modifie les informations du serveur POP et de l'utilisateur associé
         						sPOP = InfoCompte.MessageAccount.POP3_Server
         						sPOPUser = InfoCompte.MessageAccount.POP3_User_Name
         						
         						sPOP = extraitchaine(sPOPUser, 2, "@")
         						sPOPUser = extraitchaine(sPOPUser, 1, "@")
         						
         						InfoCompte.MessageAccount.POP3_User_Name = sPOPUser
         						InfoCompte.MessageAccount.POP3_Server = sPOP
         						XMLSauve(InfoCompte)
         					FIN
         					
         			FIN
         			
         		FIN
         		
         	FIN
         	
         FIN
         
