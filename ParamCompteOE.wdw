     type_code : 14
     code : |1+
      // Résumé : Lit les comptes Outlook Express disponible
      // Syntaxe :
      //LitComptes_OE ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE LitComptes_OE()
      
      
      // Parcourt la base de registre
      sCle est une chaine = RegistrePremièreSousClé("HKEY_CURRENT_USER\Software\Microsoft\Internet Account Manager\Accounts")
      TANTQUE sCle <> ""
      	
      	// Lit la clé
      	sPOP est une chaine = registrelit(sCle, "POP3 Server")
      	sPOPUser est une chaine = registrelit(sCle, "POP3 User Name")
      	sProp est une chaine = RegistreLit(sCle, "Account Name")
      	sEmail est une chaine = registrelit(sCle, "SMTP Email Address")
      	
      	// Si c'est un compte POP
      	SI sPOP <> "" ALORS
      		
      		// Le compte lu est-il déjà paramétré pour utiliser le proxy ?
      		bUtilise est un booleen = ((position(sPOPUser, ":") > 0) ou (position(sPOPUser, "@") > 0)) et (sPOP ~= sGetAdresseEcoute())
      		TableAjouteLigne(TABLE_CompteOE, sCle, bUtilise, bUtilise, sProp, sEmail, "OE")
      		
      	FIN
      	
      	// Clé suivante
      	sCle = RegistreCléSuivante(sCle)
      	
      FIN
