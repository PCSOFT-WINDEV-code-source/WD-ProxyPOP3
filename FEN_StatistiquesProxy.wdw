         	grEtiquetteCatégorie(sNomGraphe, nIndice, DateVersChaine(REQ_TailleEmailSurPeriode.Date))
         	nIndice++
         	
         FIN
         
         HAnnuleDéclaration(REQ_TailleEmailSurPeriode)
         
         // Dessin du graphe
         grDessine(sNomGraphe)
