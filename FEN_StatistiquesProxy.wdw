       internal_properties : 
       code_elements :
        type_code : 26
        p_codes :
         -
           code : |1-
            dDateDébut est une Date = datesys()
            dDateFin est une Date = datesys()
            nNumJour est un entier
            sSuperChamp est une chaine
            
            // Récupérer le nom du super champ
            sSuperChamp = ExtraitChaîne(ChampContextuel(), 1, ".")
            
            // Récupérer le numéro du jour de la semaine
            nNumJour = EntierVersJour(dDateDébut)
            
            // Semaine courante
            dDateDébut..jour -= (nNumJour-1)
            
            {sSuperChamp+<§$0008§>,indchamp} = dDateDébut
            {sSuperChamp+<§$0009§>,indchamp} = EntierVersJourEnLettre(dDateDébut)
            
            {sSuperChamp+<§$000a§>,indchamp} = dDateFin
            {sSuperChamp+<§$000b§>,indchamp} = EntierVersJourEnLettre(dDateFin)
            
            {sSuperChamp,indchamp}..Valeur = dDateDébut+tab+dDateFin
