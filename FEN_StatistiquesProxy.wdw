         grLibellé(sNomGraphe, grAfficheValeur, Vrai)
         
         // Ajout des données
         
         // Initialise la requête
         REQ_TailleEmailSurPeriode.sDateDebut = sDateDebut
         REQ_TailleEmailSurPeriode.sDatefin = sDateFin
         // Exécute la requête
         HExécuteRequête(REQ_TailleEmailSurPeriode)
