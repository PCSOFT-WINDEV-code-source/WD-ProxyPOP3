         sMaPolice = PoliceCrée(<§$0004§>, 10, iNormal)
         // Police des étiquettes
         grPoliceEtiquette(sNomGraphe, sMaPolice)
         // Police de la légende
         grPoliceLégende(sNomGraphe, sMaPolice)
         // Police du titre du graphe
         grPoliceTitre(sNomGraphe, sMaPolice)
         // Police du titre des axes
         grPoliceTitreAxe(sNomGraphe, sMaPolice)
         
         // Quadrillage horizontal
         grQuadrillage(sNomGraphe, Vrai, grAbscisse)
         
         // Affichage des valeurs
         grLibellé(sNomGraphe, grAfficheValeur, Vrai)
         
         // Ajout des données
         sDateDebut est une date = SC_SélectionPériode.SAI_DateDébut
         sDateFin est une date = SC_SélectionPériode.SAI_DateFin
         
         // Initialise la requête
         REQ_CompteEmailSurPeriode.sDateDebut = sDateDebut
         REQ_CompteEmailSurPeriode.sDatefin = sDateFin
         // Exécute la requête
         HExécuteRequête(REQ_CompteEmailSurPeriode)
         
         nIndice = 1
         // Parcours des résultats
         pour TOUT REQ_CompteEmailSurPeriode 
         	
         	grAjouteDonnée(sNomGraphe, 1, REQ_CompteEmailSurPeriode.Comptage_1)
         	grEtiquetteCatégorie(sNomGraphe, nIndice, DateVersChaine(REQ_CompteEmailSurPeriode.Date))	
         	
         	nIndice++
         FIN
         
         HAnnuleDéclaration(REQ_CompteEmailSurPeriode)
         
         // Dessin du graphe
         grDessine(sNomGraphe)
