        code : |1-
         nIndice est un entier
         sNomGraphe est une chaine
         
         sNomGraphe = <§$0000§>
         
         // Création du graphe
         grCrée(sNomGraphe, grHisto)
         // Destination du graphe
         grDestinationChamp(sNomGraphe, IMG_ImageStat)
         
         // Titre du graphe
         grTitre(sNomGraphe, <§$0001§>)
         
         // Titre de l'axe des abscisses
         grTitreAxe(sNomGraphe, <§$0002§>, grAbscisse)
         
